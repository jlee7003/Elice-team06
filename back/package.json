{
    "name": "back",
    "version": "0.0.0",
    "scripts": {
        "build": "npx ts-node build.ts",
        "lint": "node_modules/.bin/eslint",
        "dev": "nodemon"
    },
    "nodemonConfig": {
        "watch": [
            "src"
        ],
        "ext": "ts, html",
        "ignore": [
            "src/public"
        ],
        "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src"
    },
    "engines": {
        "node": ">=8.10.0"
    },
    "dependencies": {
        "@types/uuid": "^8.3.4",
        "bcrypt": "^5.0.1",
        "command-line-args": "^5.2.1",
        "cors": "^2.8.5",
        "dotenv": "^16.0.2",
        "express": "^4.18.1",
        "express-async-errors": "^3.1.1",
        "helmet": "^6.0.0",
        "http-status-codes": "^2.2.0",
        "jet-logger": "^1.2.6",
        "jsonfile": "^6.1.0",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^6.6.2",
        "morgan": "^1.10.0",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/command-line-args": "^5.2.0",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.14",
        "@types/find": "^0.2.1",
        "@types/fs-extra": "^9.0.13",
        "@types/jsonfile": "^6.1.0",
        "@types/jsonwebtoken": "^8.5.9",
        "@types/morgan": "^1.9.3",
        "@types/node": "^18.7.22",
        "@typescript-eslint/eslint-plugin": "^5.38.1",
        "@typescript-eslint/parser": "^5.38.1",
        "eslint": "^8.24.0",
        "eslint-plugin-node": "^11.1.0",
        "find": "^0.3.0",
        "fs-extra": "^10.1.0",
        "nodemon": "^2.0.20",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.1.0",
        "typescript": "^4.8.3"
    }
}
